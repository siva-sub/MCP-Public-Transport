# Required - LTA DataMall API Key
LTA_ACCOUNT_KEY=your_lta_api_key_here

# Optional - OneMap API Token for higher rate limits
ONEMAP_TOKEN=your_onemap_token_here

# Cache settings
CACHE_DURATION=300

# Logging
LOG_LEVEL=info
LOG_FILE=

# Performance settings  
REQUEST_TIMEOUT=30000
MAX_WALK_DISTANCE=1000

# Features
ENABLE_CROWD_PREDICTION=true
ENABLE_COST_OPTIMIZATION=true
ENABLE_ACCESSIBILITY_ROUTES=true
